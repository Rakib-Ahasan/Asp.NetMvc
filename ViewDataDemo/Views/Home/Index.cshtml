@using ViewDataDemo.Models

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<h2>@ViewData["Message"]</h2>
<h2>@ViewData["CurrentTime"]</h2>

<ul>
    @{
        foreach (string i in (List<string>)ViewData["SportsList"])
        {
            <li>@i</li>
        }
    }
</ul>
@{
    var a = (Employee)ViewData["employee"];
}

<p>@a.Id</p>
<p>@a.Name</p>
<p>@a.Designation</p>