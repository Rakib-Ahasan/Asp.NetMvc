@model IEnumerable<MultipleModelDataInAsingleView.Models.Student>

<h1>Students Data</h1>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Class</th>
            <th>Fees</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.name</td>
                <td>@item.gender</td>
                <td>@(item.@class)</td>
                <td>@item.fees</td>
            </tr>
        }
    </tbody>
</table>