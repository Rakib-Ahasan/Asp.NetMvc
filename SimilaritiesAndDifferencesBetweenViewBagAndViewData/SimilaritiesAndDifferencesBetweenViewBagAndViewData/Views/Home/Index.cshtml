
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<h3>@ViewData["Message1"]</h3>
<h3>@ViewBag.Message2</h3>

<h3>@ViewData["CurrentDate1"]</h3>
<h3>@ViewBag.CurrentDate2</h3>

<h3>@ViewData["Game1"]</h3>
<h3>@ViewBag.Game2</h3>

<ul>
    @{
        foreach (string i in (string[])ViewData["Game1"])
        {
            <li>@i</li>
        }
    }
</ul>

<ul>
    @{
        foreach (string i in ViewBag.Game2)
        {
            <li>@i</li>
        }
    }
</ul>